<?xml version="1.0" encoding="UTF-8"?>
<sequence name="BankTransactionsIteratorSequence" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log level="full"/>
    <iterate expression="//TransactionsStatuses" id="BankTransactionInterator" xmlns:ns="http://ws.wso2.org/dataservice">
        <target>
            <sequence>
                <property expression="//TransactionReference" name="TransactionReference" scope="default" type="STRING"/>
                <property expression="//BankTransactionRef" name="BankTransactionRef" scope="default" type="STRING"/>
                <property expression="//BankTransactionCompleted" name="BankTransactionCompleted" scope="default" type="STRING"/>
                <store messageStore="BankTransactionStore" sequence="GetInquiryTransactionSequence"/>
            </sequence>
        </target>
    </iterate>
</sequence>
